Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.buffered: true
URxvt.depth: 32
#URxvt*geometry: 75x20

URxvt.visualBell: false

URxvt*font: xft:gohufont:regular:size=10
